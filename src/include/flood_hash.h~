#ifndef FLOOD_HASH_H
#define FLOOD_HASH_H

struct hash_bucket{
  bool occuiped = false;
  Vector2I cord;
};

struct flood_hashmap{
  hash_bucket *buffer;
  int cap;
};

flood_hashmap create_flood_map(int cap);

void destroy_flood_map(flood_hashmap *map);

void hash_add(Vector2I cords,flood_hashmap *map);

bool hash_look(Vector2I cords,flood_hashmap *map);



#endif
